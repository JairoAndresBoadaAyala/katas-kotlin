<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="ThemeHelloAndroid" parent="Theme.MaterialComponents.Light.NoActionBar">
        <item name="android:forceDarkAllowed">false</item>
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/goldF6C700</item>
        <item name="colorPrimaryVariant">@color/white</item>
        <item name="android:windowLightStatusBar">true</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/goldF6C700</item>
        <item name="colorSecondaryVariant">@color/goldF6C700</item>
        <item name="colorOnSecondary">@color/goldF6C700</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor">@color/goldF6C700</item>
    </style>
</resources>